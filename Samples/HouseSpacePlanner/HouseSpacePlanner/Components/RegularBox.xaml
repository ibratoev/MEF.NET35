<h:SpaceObject x:Class="HouseSpacePlanner.Components.RegularBox"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:h="clr-namespace:HouseSpacePlanner;assembly=HouseSpacePlannerCommon"    
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">

    <Rectangle Width="50" Height="50" Stroke="#FF5C5C5C"  >
        <Rectangle.Fill>
            <LinearGradientBrush EndPoint="0.245,0.999" StartPoint="0.947,0.034">
                <GradientStop Color="#FF6E6E6E" Offset="0"/>
                <GradientStop Color="#FFCACACA" Offset="1"/>
            </LinearGradientBrush>
        </Rectangle.Fill>
    </Rectangle>

</h:SpaceObject>
